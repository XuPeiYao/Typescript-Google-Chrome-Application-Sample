{
  "name": "TypeScript Google Chrome Application",
  "description": "My first Chrome App.",
  "version": "0.1",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": [
        "libraries/nativeExtensions.js",
        "libraries/chromeExtensions.js",
        "startup.js"
      ]
    }
  },
  "permissions": [
    "<all_urls>"
  ],
  "icons": {
    "512": "images/icons/app.png"
  }
}